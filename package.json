{
  "name": "ciphare",
  "version": "0.2.5",
  "private": true,
  "scripts": {
    "flask-dev": "cross-env FLASK_DEBUG=1 pip3 install -r requirements.txt && python3 -m flask --app api.app run -p 5328 --reload",
    "flask-prod": "pip3 install -r requirements.txt && gunicorn --bind 0.0.0.0:5328 api.app:flask_app",
    "next-prod": "next start",
    "next-dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev": "concurrently \"npm run next-dev\" \"npm run flask-dev\"",
    "prod": "concurrently \"npm run next-prod\" \"npm run flask-prod\""
  },
  "dependencies": {
    "@upstash/redis": "^1.34.3",
    "concurrently": "^9.1.0",
    "next": "15.0.3",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}

{
  "version": 2,
  "buildCommand": "cd source1/my-encryption-app && npm install && npm run build",
  "devCommand": "cd source1/my-encryption-app && npm run dev",
  "installCommand": "cd source1/my-encryption-app && npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "api/*.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 